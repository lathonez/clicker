    "devDependencies": {
        "@angular/cli": "1.1.2",
        "@ionic/app-scripts": "1.3.7",
        "@ionic/cli-plugin-ionic-angular": "1.3.1",
        "@ionic/cli-plugin-cordova": "1.4.0",
        "@types/jasmine": "2.5.41",
        "@types/node": "7.0.4",
        "codecov": "2.2.0",
        "connect": "3.6.2",
        "ionic-mocks": "0.10.1",
        "jasmine-core": "2.6.2",
        "jasmine-reporters": "2.2.1",
