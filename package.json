        "serve-static": "1.12.3",
        "ts-node": "3.0.4",
        "tslint": "5.3.2",
        "tslint-eslint-rules": "4.1.1",
        "typescript": "2.3.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lathonez/clicker.git"
